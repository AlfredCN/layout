<UITableViewCell
    detailTextLabel.text="Subtitle #{row}"
    detailTextLabel.textColor="#aaa"
    height="auto + 20"
    reuseIdentifier="cell"
    style="subtitle"
    textLabel.text="Title #{row}">

    <UIImageView
        height="auto"
        image="{image}"
        right="100% - 20"
        tintColor="#999"
        top="20"
        width="auto"
    />
</UITableViewCell>
